<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.mygatoel">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_mygatoel"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyGatoel"
        android:usesCleartextTraffic = "true"
        tools:targetApi="m">
        <activity android:name=".Pasien.Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Guest.Login"></activity>
        <activity android:name=".Guest.Register"></activity>
        <activity android:name=".Pasien.MainPasien"></activity>
        <activity android:name=".Pasien.PendaftaranOnline"></activity>
        <activity android:name=".Pasien.JadwalPraktikDokter"></activity>
        <activity android:name=".Pasien.TutorialPendaftaranOnline"></activity>
        <activity android:name=".Admin.MainAdmin"></activity>
        <activity android:name=".Admin.TambahDokter"></activity>
        <activity android:name=".Pasien.StatusPendaftaran"></activity>
        <activity android:name=".Admin.UbahInformasiDokter"></activity>
        <activity android:name=".Pasien.DetailPendaftaranPs"></activity>
        <activity android:name=".Admin.DetailPendaftaranAd"></activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>